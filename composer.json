{
    "name": "hosomikai/laravel-ezship",
    "description": "台灣便利配ezship物流API",
    "type": "library",
    "require": {
        "php": "^7.0|^8.0"
    },
    "authors": [
        {
            "name": "hosomikai",
            "email": "showking111@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Hosomikai\\Ezship\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hosomikai\\Ezship\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hosomikai\\Ezship\\EzshipServiceProvider"
            ],
            "aliases": {
                "Ezship": "Hosomikai\\Ezship\\Facades\\Ezship"
            }
        }
    }
}